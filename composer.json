{
    "name": "mrandmrssmith/idempotent-symfony-messenger-consumer-bundle",
    "description": "idempotent-consumer-bundle",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "symfony/messenger": ">=4.4",
        "symfony/serializer": ">=4.4",
        "symfony/property-access": ">=4.4",
        "symfony/symfony": ">=4.4",
        "mrandmrssmith/idempotent-consumer-bundle": "dev-main"
    },
    "authors": [
        {
            "name": "Michal Boryczko",
            "email": "michal.boryczko@smithhotels.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MrAndMrsSmith\\IdempotentConsumerSymfonyMessengerBundle\\": "/src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": ">=8.5",
        "roave/security-advisories": "dev-master",
        "symplify/easy-coding-standard": "^12.1",
        "vimeo/psalm": "^4.30||^5.22"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/mrandmrssmith/idempotent-consumer-bundle.git"
        }
    ]
}
